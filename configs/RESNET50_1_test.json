{
  "exp_name": "RESNET50_1",
  "agent": "VOCACTIVITY_Agent",
  "model": "ResNet50",
  "single_label": true,
  "classes_num": 11,
  "data_loader": "VOCDataLoader",
  "img_root": "data/images/",
  "train_pickle": "data/train_data.pickle",
  "valid_pickle": "data/valid_data.pickle",
  "resize": [128, 128],
  "person_num": 1,
  "crop_images": true,
  "optimizer": "Adam",
  "learning_rate": 1e-5,
  "mode": "test",
  "best_file": "RESNET50_1-30-0.54.h5",
  "seed": 1337,
  "num_classes": 11,
  "num_workers": 1,
  "batch_size": 2,
  "max_epoch": 100,
  "verbose_training": 1,
  "use_scheduler": false,
  
  "callbacks":{
    "checkpoint_monitor": "val_acc",
    "checkpoint_mode": "max",
    "checkpoint_save_best_only": true,
   "checkpoint_save_weights_only": true,
    "checkpoint_verbose": true,
    "tensorboard_write_graph": true
  }
}
